pipeline {
  agent any
  stages {
    stage('build') {
      parallel {
        stage('build') {
          steps {
            echo '123'
          }
        }

        stage('hh') {
          steps {
            echo '1'
          }
        }

      }
    }

    stage('kjnkjn') {
      parallel {
        stage('kjnkjn') {
          steps {
            echo '0'
          }
        }

        stage('gl') {
          steps {
            echo 'ggl'
          }
        }

      }
    }

    stage('end') {
      steps {
        echo 'gg'
      }
    }

  }
}